UUID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx   /           btrfs   subvol=root,compress=zstd:3,discard=async,commit=60,usebackuproot   0 0 
UUID=XXXX-XXXX                              /boot/efi   vfat    umask=0077,shortname=winnt    0 2 
UUID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx   /home       btrfs   subvol=home,compress=zstd:3,discard=async,commit=60,usebackuproot   0 0 
